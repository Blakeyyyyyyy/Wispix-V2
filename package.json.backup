{
  "name": "wispix-ai-automation-platform",
  "version": "1.0.0",
  "description": "Wispix AI Automation Platform - Full Stack Application",
  "main": "backend/dist/index.js",
  "scripts": {
    "build": "npm install && tsc",
    "start": "node backend/dist/index.js",
    "dev": "cd backend && PORT=3001 npm run dev",
    "dev:frontend": "cd frontend && PORT=3000 npm start",
    "dev:backend": "cd backend && PORT=3001 npm run dev",
    "dev:both": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:cline": "cross-env USE_CLINE=1 nodemon --signal SIGTERM backend/dist/index.js",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build:all": "npm run build && cd frontend && npm run build",
    "test": "cd backend && npm test",
    "postinstall": "cd backend && prisma generate",
    "db:setup": "cd backend && npm run db:setup",
    "db:init": "cd backend && npx prisma db push && npx prisma generate",
    "verify:agents": "tsx backend/scripts/verifyAgents.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.51.0",
    "@types/node-fetch": "^2.6.12",
    "@types/nodemailer": "^6.4.17",
    "@types/redis": "^4.0.10",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.12.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "googleapis": "^156.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "pg": "^8.16.3",
    "uuid": "^9.0.1",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@prisma/client": "^6.13.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "concurrently": "^8.2.2",
    "cross-env": "^10.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "18.x"
  }
}
